{
  "name": "www-kevinmitchell-io",
  "version": "1.0.0",
  "description": "",
  "main": "kevinmitchell.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "run-p build watch serve",
    "build": "run-s clean sass move",
    "clean": "rimraf dist",
    "sass": "node-sass --output-style expanded src/style/index.scss dist/style/index.css && node-sass --output-style compressed src/style/index.scss dist/style/index.min.css",
    "move": "copyfiles src/index.html dist --up 1",
    "watch": "onchange src -- run-p build",
    "serve": "browser-sync start --no-notify -s test --ss dist -f dist"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "banner-cli": "^0.12.0",
    "browser-sync": "^2.23.7",
    "copyfiles": "^2.0.0",
    "css-reset-and-normalize-sass": "^0.1.2",
    "milligram": "^1.3.0",
    "minireset.css": "0.0.3",
    "node-sass": "^4.9.0",
    "normalize-scss": "^7.0.1",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.2",
    "onchange": "^3.3.0",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.1"
  }
}
